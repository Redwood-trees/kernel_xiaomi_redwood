# Fingerprint
CONFIG_INPUT_FINGERPRINT=y
CONFIG_FINGERPRINT_GOODIX_TEE=m
CONFIG_FINGERPRINT_XIAOMI_FP=m

# Haptics
# CONFIG_INPUT_QCOM_HV_HAPTICS is not set
# CONFIG_LEDS_QPNP_VIBRATOR_LDO is not set
CONFIG_INPUT_AW86927_HAPTIC=m

# IO Schedulers
CONFIG_CFQ_GROUP_IOSCHED=y

# IR
CONFIG_IR_SPI=m
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# KernelSU
CONFIG_KSU=y

# Leds
CONFIG_LEDS_QTI_FLASH=m

# MMC
# CONFIG_MMC is not set

# Power supply
# CONFIG_BACKLIGHT_QCOM_SPMI_WLED is not set
CONFIG_QTI_BATTERY_CHARGER=y
CONFIG_AI_RSOC_M20=y
CONFIG_BQ2597X=y
CONFIG_BQ_CLOUD_AUTHENTICATION=y
CONFIG_BQ_FG_1S=y
CONFIG_BQ_FUEL_GAUGE=y
CONFIG_REDWOOD_FOR_BUILD=y

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m

# Touchscreen
# CONFIG_TOUCHSCREEN_FTS is not set
# CONFIG_TOUCHSCREEN_NT36XXX is not set
# CONFIG_TOUCHSCREEN_ST is not set
CONFIG_TOUCHSCREEN_FOCALTECH_3680=m
CONFIG_TOUCHSCREEN_GOODIX_BRL_9916_M20=m
CONFIG_TOUCHSCREEN_QGKI_GOODIX=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m

# USB
CONFIG_USB_F_DTP=m

# USB Audio
# CONFIG_QCOM_FSA4480_I2C is not set

# WiFi
CONFIG_ICNSS2=m
# CONFIG_ICNSS2_RESTART_LEVEL_NOTIF is not set
CONFIG_CNSS2=m
CONFIG_CNSS_QCA6750=y
CONFIG_QCA_CLD_WLAN=m
